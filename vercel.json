{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "server/index.js": {
      "runtime": "nodejs22.x"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/server/index.js" },
    { "src": "/(.*)", "dest": "/dist/public/index.html" }
  ]
}
