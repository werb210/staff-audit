  "scripts": {
    "dev": "node ./node_modules/.bin/tsx --watch server/index.ts",
    "build": "npm run build:client",
    "start": "node ./node_modules/.bin/tsx server/index.ts",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "build:client": "vite build",
    "build:server": "node ./node_modules/.bin/tsc -p tsconfig.json || echo \"(ts build optional for tsx runtime)\"",
    "build:client:fast": "vite build --minify=false --sourcemap=false",
    "deploy": "npm run build:client:fast && npm run start",
    "typecheck": "node ./node_modules/.bin/tsc -p tsconfig.json",
    "transcripts:once": "node server/workers/transcripts.worker.mjs",
    "transcripts:watch": "nodemon server/workers/transcripts.worker.mjs",
    "start:real": "npx tsx server/index.ts",
    "dev:real": "npx tsx --watch server/index.ts",
    "start:safe": "npx tsx server/bootstrap/safe-server.ts",
    "dev:safe": "npx tsx --watch server/bootstrap/safe-server.ts",
    "test": "echo \"(no tests)\" && exit 0",
    "clean": "rimraf dist build .vite .cache || true",
    "serve:client": "node ./node_modules/.bin/vite preview --host 0.0.0.0 --port 5173",
    "predeploy": "npm run typecheck && (npm run lint || echo 'no lint script') && echo OK",
    "test:auth:unit": "vitest run tests/auth_cookie_header.spec.ts",
    "test:auth": "npm run -s test:auth:unit",
    "check:dup": "node scripts/dup_guard.mjs",
    "build:strict": "npm run check:dup && npm run build",
    "postbuild:client": "mkdir -p client/dist && cp -r dist/public/* client/dist/",
    "prepare": "node scripts/prepare-husky.cjs",
    "seed:products": "tsx scripts/sync-products.ts",
    "postdeploy": "npm run seed:products && npm run seed:docs || true",
    "seed:docs": "tsx scripts/seed-required-docs.ts",
    "e2e:server": "jest --config=jest.config.cjs",
    "e2e:lender:mock": "ts-node tools/mock-lender.ts",
    "e2e:all:staff": "npm-run-all -p e2e:lender:mock e2e:server"
  },
