{
  "name": "@financial-platform/staff-portal",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5002",
    "build": "vite build",
    "build:safe": "npm run check-duplicates && vite build",
    "postbuild": "node -e \"const fs=require('fs');fs.writeFileSync('dist/build.json', JSON.stringify({time:new Date().toISOString(), rand:Math.random().toString(36).slice(2)},null,2))\"",
    "build:force": "npm run check-duplicates --fix && vite build",
    "preview": "vite preview",
    "check-duplicates": "node scripts/check-duplicates.js",
    "check-duplicates:fix": "node scripts/check-duplicates.js --fix",
    "check-duplicates:strict": "node scripts/check-duplicates.js --strict",
    "test:e2e": "playwright test",
    "test:e2e:report": "playwright test && echo '\nReport written to tests/REPORT.md and playwright-report/index.html\n'",
    "test:install": "playwright install --with-deps"
  },
  "dependencies": {
    "@financial-platform/shared": "workspace:*",
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "^1.2.4",
    "@radix-ui/react-alert-dialog": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.4",
    "@radix-ui/react-button": "^1.1.3",
    "@radix-ui/react-card": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.5",
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-form": "^0.1.0",
    "@radix-ui/react-input": "^1.1.3",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-progress": "^1.1.3",
    "@radix-ui/react-select": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.3",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.4",
    "@radix-ui/react-textarea": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.7",
    "@radix-ui/react-tooltip": "^1.2.0",
    "@tanstack/react-query": "^5.60.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.453.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.55.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "wouter": "^3.3.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.6.3",
    "vite": "^5.4.19"
  }
}
