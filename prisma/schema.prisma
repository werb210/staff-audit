  stage                                                    pipeline_stage?             @default(New)
  contact_id                                               String?                     @db.Uuid
  utm_source                                               String?
  utm_medium                                               String?
  utm_campaign                                             String?
  gclid                                                    String?
  ad_platform                                              String?
  ad_campaign_id                                           String?
  ad_group_id                                              String?
  ad_id                                                    String?
  funded                                                   Boolean?                    @default(false)
  business_name                                            String?                     @db.VarChar
